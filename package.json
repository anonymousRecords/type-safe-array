{
  "name": "type-safe-array",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "dev:docs": "pnpm --filter docs run dev",
    "build:docs": "pnpm --filter docs run build",
    "start:docs": "pnpm --filter docs run start",
    "lint:docs": "pnpm run lint --filter=docs",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "test:all": "turbo run test",
    "build:test": "turbo run build test"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.0.12",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/*",
        "eslint",
        "typescript"
      ]
    }
  }
}
